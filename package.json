{
  "name": "jq-web",
  "version": "0.5.2",
  "description": "a hack that makes jq run in the browser with emscripten.",
  "main": "dist/jq.wasm.js",
  "files": [
    "dist/jq.wasm.d.ts",
    "dist/jq.wasm.js",
    "dist/jq.wasm.wasm"
  ],
  "scripts": {
    "copy-to-src": "rm -f src/*.js && cp node_modules/jq-web/*.js src/",
    "copy-to-dist": "cp node_modules/jq-web/jq.wasm.wasm dist/",
    "reformat": "esbuild src/jq.wasm.js --platform=node --format=cjs --target=node18 > dist/reformatted-jq.wasm.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fiatjaf/jq-web.git"
  },
  "keywords": [
    "emscriptem",
    "jq",
    "json",
    "filter",
    "path"
  ],
  "author": "fiatjaf",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/fiatjaf/jq-web/issues"
  },
  "homepage": "https://github.com/fiatjaf/jq-web#readme",
  "devDependencies": {
    "esbuild": "^0.17.5",
    "jq-web": "^0.5.1"
  }
}
